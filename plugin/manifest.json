{
  "manifest_version": 2,
  "version": "0.0.3",
  "name": "Highlights",
  "description": "Highlights the most meaningful words on a web page",
  "permissions": [
    "activeTab",
    "https://*/",
    "http://*/",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "js": ["./vendors/mark.min.js", "./vendors/axios.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}